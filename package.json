{
  "name": "node-express-seed",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prestart": "",
    "test": "NODE_ENV=test node ./node_modules/.bin/jasmine-node --color --verbose --forceexit --captureExceptions --test-dir test",
    "test:cover": "istanbul cover --include-all-sources jasmine-node --handle-sigint test",
    "clean": "rm -fr build",
    "build": "babel app --out-dir build --source-maps inline --copy-files && mv build/bin/www.js build/bin/www",
    "clean:build": "npm run clean && npm run build",
    "clean:start": "npm run clean && npm run build && DEBUG=lair,api,error NODE_ENV=local nodemon --ignore apidoc/ ./build/bin/www",
    "start": "DEBUG=lair,api,error NODE_ENV=local nodemon --exec babel-node --ignore apidoc/ -- ./app/bin/www.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator",
      "syntax-async-functions"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "helmet": "^1.0.2",
    "kerberos": "0.0.17",
    "moment": "^2.17.1",
    "mongodb": "^2.1.3",
    "mongoose": "^4.3.4",
    "mongoose-validator": "^1.2.4",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "istanbul": "latest",
    "jasmine-node": "^1.14.5",
    "mongodb": "latest",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.3",
    "supertest": "^1.1.0"
  }
}
